<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:translationZ="100dp"
    tools:context=".activities.events.EventsActivity">

    <Button
        android:id="@+id/button2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/aauser_name_icon_bg"
        android:fontFamily="@font/roboto_black"
        android:text="DÅ "
        android:textColor="@color/white"
        android:textSize="20dp"
        android:translationZ="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Toolbar
        android:layout_width="180dp"
        android:layout_height="70dp"
        android:background="@drawable/aanavigation_btn_bg"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="30dp"
        android:paddingEnd="20dp"
        android:paddingStart="0dp"
        android:translationZ="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/time_table_iconn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:padding="2dp"
                android:src="@drawable/baseline_access_time_24"
                android:background="@drawable/aanavigation_icon_btn_bg"/>
            <ImageButton
                android:id="@+id/events_iconn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:padding="2dp"
                android:src="@drawable/baseline_table_rows_24"
                android:background="@drawable/aanavigation_icon_btn_bg"/>
            <ImageButton
                android:id="@+id/assessments_iconn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:padding="2dp"
                android:src="@drawable/baseline_notifications_none_24"
                android:background="@drawable/aanavigation_icon_btn_bg"/>
        </LinearLayout>
    </Toolbar>


</androidx.constraintlayout.widget.ConstraintLayout>